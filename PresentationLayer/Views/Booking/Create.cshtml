@model PresentationLayer.Models.CreateBooking

@{
    ViewBag.Title = "Overview";
    Layout = "~/Views/Home/_AdminNavigation.cshtml";
}

<h2 style="padding-left:20px;padding-bottom:20px">Add a new resource booking</h2>

<div class="container">

     <ul class="nav nav-pills">
    <li class="active"><a data-toggle="pill" href="#single">Single booking</a></li>
    <li><a data-toggle="pill" href="#block">Block booking</a></li>
    <li><a data-toggle="pill" href="#group">Group booking</a></li>
  </ul>

  <div class="tab-content">
    <div id="single" class="tab-pane fade in active">
        @Html.Partial("_basicBooking", Model)
    </div>
    <div id="block" class="tab-pane fade">
        @Html.Partial("_blockBooking", Model)
    </div>
    <div id="group" class="tab-pane fade">
        @Html.Partial("_groupBooking", Model)
    </div>
  </div>

</div>


